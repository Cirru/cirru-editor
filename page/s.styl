
#editor
  background: hsl(240,80%,80%)
  height: 500px
  width: 800px
  font-family 'wenquanyi micro hei mono'
  padding 6px
  color white

  div
    outline none
    display inline-block
    border-color white
    border-style solid
    border-width 0px 0px 1px 0px
    margin 0px 4px
    padding 0px 1px

  section
    border-color white
    border-style solid
    border-width 0px 0px 0px 1px
    margin-left 14px